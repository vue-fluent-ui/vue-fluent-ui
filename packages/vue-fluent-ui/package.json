{
  "name": "vue-fluent-ui",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "module": "./dist/vue-fluent-ui.js",
  "main": "./dist/vue-fluent-ui.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vue-fluent-ui.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/vue-fluent-ui.cjs"
      }
    },
    "./style.css": "./dist/vue-fluent-ui.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "build:publish": "pnpm run build && pnpm publish ",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "typescript": "~5.8.3",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@vueuse/core": "^13.6.0",
    "lodash": "^4.17.21"
  }
}